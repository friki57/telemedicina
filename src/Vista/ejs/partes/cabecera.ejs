<% if(datos.usuario==undefined) {%>
  <%- include("./cabeceraIni.ejs") %>
  <% } else {%>

    <% if(datos.usuario.tipo == "doctor") {%>
      <%- include("./cabeceraDoctor.ejs") %>
      <% }%>
    <% if(datos.usuario.tipo == "centro") {%>
      <%- include("./cabeceraCentro.ejs") %>
      <% }%>
    <% if(datos.usuario.tipo == "admin") {%>
      <%- include("./cabeceraAdmin.ejs") %>
      <% }%>
    <% if(datos.usuario.tipo == "Paciente" && datos.usuario.hash == 0 ) {%>
      <%- include("./cabeceraPaciente.ejs") %>
      <% }%>
    <% if(datos.usuario.tipo=="Paciente" && datos.usuario.hash != 0 ) {%>
      <%- include("./cabeceraPacienteNoVerificado.ejs") %>
        <% }%>
  <% }%>
